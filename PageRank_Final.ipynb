    "import csv\n",
    "import requests\n",
    "import time\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "def search(keyword):\n",
    "    url = 'https://www.google.com/search?q=' + keyword\n",
    "    headers = {'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/58.0.3029.110 Safari/537.3'}\n",
    "    response = requests.get(url, headers=headers)\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    results = soup.select('.tF2Cxc')\n",
    "\n",
    "    ranked_results = []\n",
    "    for i, result in enumerate(results):\n",
    "        title_element = result.select_one('.DKV0Md')\n",
    "        if title_element is not None:\n",
    "            title = title_element.text\n",
    "        else:\n",
    "            title = ''\n",
    "        link_element = result.select_one('.yuRUbf a')\n",
    "        if link_element is not None and 'href' in link_element.attrs:\n",
    "            link = link_element['href']\n",
    "        else:\n",
    "            link = ''\n",
    "        description_element = result.select_one('.IsZvec')\n",
    "        if description_element is not None:\n",
    "            description = description_element.text\n",
    "        else:\n",
    "            description = ''\n",
    "        relevance = (len(results) - i) / len(results)\n",
    "        ranked_results.append({'title': title, 'link': link, 'description': description, 'relevance': relevance})\n",
    "\n",
    "    return ranked_results\n",
    "\n",
    "def check_bookmarks(links):\n",
    "    # Read the bookmarks file\n",
    "    with open('bookmarks.csv', 'r', newline='', encoding='utf-8') as file:\n",
    "        reader = csv.DictReader(file)\n",
    "        bookmarks = [row['Link'] for row in reader]\n",
    "\n",
    "    # Check each link against the bookmarks\n",
    "    for link in links:\n",
    "        if link in bookmarks:\n",
    "            yield 1\n",
    "        else:\n",
    "            yield 0\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    keyword = input('Enter a keyword to search: ')\n",
    "    results = search(keyword)\n",
    "\n",
    "    # Write results to a CSV file\n",
    "    with open('C:/Users/aryat/search_results.csv', 'w', newline='', encoding='utf-8') as file:\n",
    "        writer = csv.writer(file)\n",
    "        writer.writerow(['Title', 'Link', 'Description', 'Relevance', 'Time Spent'])\n",
    "\n",
    "        for result in results:\n",
    "            start_time = time.time()\n",
    "\n",
    "            # Process the link\n",
    "            response = requests.get(result['link'])\n",
    "            
    "\n",
    "            end_time = time.time()\n",
    "            time_spent = round(end_time - start_time, 2)\n",
    "\n",
    "            writer.writerow([result['title'], result['link'], result['description'], result['relevance'], time_spent])\n",
    "\n",
    "    # Print results to console\n",
    "    print('Top search results for \"' + keyword + '\":')\n",
    "    for i, result in enumerate(results):\n",
    "        print(str(i+1) + '. ' + result['title'])\n",
    "        print('   ' + result['link'])\n",
    "        print('   ' + result['description'])\n",
    "        print('   Relevance: ' + str(result['relevance']))\n",
    "        print()"
   ]
